@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityUser>

<div class="container">
    <table class='table table-striped'>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
        }
    </table>
</div>
